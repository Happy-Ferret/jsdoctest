{
  "name": "jsdoctest",
  "version": "1.4.0",
  "description": "Run jsdoc examples as doctests",
  "main": "lib/index.js",
  "bin": "bin/jsdoctest",
  "scripts": {
    "bin": "./bin/jsdoctest",
    "test": "JSDOCTEST_DISABLE=true mocha --require blanket -R mocha-spec-cov-alt",
    "coverage": "JSDOCTEST_DISABLE=true mocha --require blanket -R html-cov > coverage.html"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/yamadapc/jsdoctest.git"
  },
  "keywords": [
    "test",
    "jsdoc",
    "doctests",
    "testing",
    "workflow"
  ],
  "author": "Pedro Tacla Yamada <tacla.yamada@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yamadapc/jsdoctest/issues"
  },
  "homepage": "https://github.com/yamadapc/jsdoctest",
  "dependencies": {
    "commander": "^2.5.0",
    "dox": "^0.5.3",
    "js-beautify": "^1.5.4",
    "lodash": "^2.4.1",
    "should": "^4.3.0"
  },
  "devDependencies": {
    "blanket": "^1.1.6",
    "mocha": "^2.0.1",
    "mocha-spec-cov-alt": "^0.2.3"
  },
  "config": {
    "blanket": {
      "data-cover-never": [
        "node_modules"
      ],
      "pattern": [
        "lib",
        "bin"
      ],
      "spec-cov": {
        "threshold": 70,
        "lcovOutput": "lcov"
      }
    }
  }
}
